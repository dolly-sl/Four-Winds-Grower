{% if settings['social-facebook'] != blank %}
  <a
    {% if className %}
      class="{{className}}"
    {% endif %}
    target="_blank"
    href="{{settings.social-facebook}}"
    title="Facebook"
  >
    <i class="fa fa-facebook-square"></i>
  </a>
{% endif %}
