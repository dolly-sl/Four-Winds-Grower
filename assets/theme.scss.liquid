#search-page {
  & {
    padding-bottom: 40px;
    border-top: 1px solid #e3dad0;
  }
  .no-search {
    margin-top: 60px;
    text-align: center;
  }
  .search-options {
    & {
      padding: 20px 0;
      border-bottom: 1px solid #e3dad0;
    }
    form {
      & {
        position: relative;
        display: block;
        width: 100%;
      }
      input[type="text"] {
        padding-right: 20px;
        width: 100%;
        border: 1px solid #d3d3d3;
        height: 35px;
        padding-left: 20px;
      }
      button {
        background: transparent;
        border: none;
        position: absolute;
        font-size: 18px;
        color: $colorBodyBlack;
        top: 50%;
        right: 5px;
        margin-top: -11px;
      }
    }
    .styled-select {
      & {
        position: relative;
        top: auto;
        right: auto;
        @include transform(translate(0));
        margin-top: 8px;
      }
      i {
        font-size: 14px;
      }
    }
    .filter-col {
      & {
        text-align: right;
      }
    }
  }
  .search-results {
    h6 {
      margin-top: 20px;
      text-align: center;
      margin-bottom: 20px;
    }
    .blog-article {
      margin-top: 40px;
      margin-bottom: 0;
    }
  }
}

.collectionHeadRow {
  background-image: url("https://cdn.shopify.com/s/files/1/2336/3219/files/fourwinds-slider_1.jpg?8415897413456501271");
  padding-top: 30px;
  padding-bottom: 30px;
  background-size: cover;
  overflow: hidden;
  &:after {
    content: "";
    position: absolute;
    top: 115px;
    left: 0;
    width: 100%;
    height: 162px;
    background-color: rgba(0, 0, 0, 0.5);
  }
  img {
    display: block;
    margin: auto;
    position: relative;
    z-index: 1;
  }
  h1,
  h4 {
    font-size: 20px;
    display: block;
    margin: auto;
    text-align: center;
    margin-top: 30px;
    position: relative;
    z-index: 2;
    color: white;
  }
}

