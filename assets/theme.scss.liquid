#contact {
  .top-info {
    p {
      font-family: $fontAvenirMedium;
    }
    .top-message {
      margin-bottom: 25px;
    }
    .contact-info {
      & {
        display: block;
        margin-bottom: 20px;
      }
      &:last-of-type {
        margin: 0;
      }
      i,
      p {
        line-height: 25px;
        display: inline-block;
      }
      i {
        margin-right: 5px;
        min-width: 25px;
        color: $colorOrange;
        font-size: 20px;
        position: relative;
        top: 1px;
      }
    }
    .bottom-message {
      margin-top: 25px;
    }
    .social {
      & {
        border-top: 1px solid #e3dad0;
        padding-top: 20px;
        margin-top: 40px;
        margin-bottom: 50px;
      }
      a {
        margin-right: 15px;
      }
      a i {
        font-size: 30px;
        color: #564f47;
      }
    }
  }
}

#submit-story {
  .top-info {
    & {
      margin-bottom: 40px;
    }
    strong {
      font-family: $fontSavuBoldCondensed;
      font-size: 14px;
      font-weight: 400;
    }
  }
  form {
    ul {
      padding: 0;
      margin: 0;
      list-style: none;
    }
    input,
    textarea {
      margin-bottom: 20px;
    }
    .form-error-message {
      & {
        font-size: 14px;
        margin-bottom: 15px;
        margin-top: -20px;
        color: $colorOrange;
      }
      img {
        display: none;
      }
    }
    .form-line {
      position: relative;
    }
    .inputContainer {
      height: 40px !important;
    }
    .qq-upload-button {
      display: none !important;
    }
    .label-type,
    .file-name {
      margin-right: 60px;
      font-family: $fontAvenirMedium;
      font-size: 14px;
      color: #443e37;
      letter-spacing: 0;
    }
    .file-name {
      margin-right: 0;
      margin-left: 20px;
    }
    .qq-upload-list {
      margin-top: 10px;
    }
    .qq-uploader {
      .qq-upload-file {
        margin-right: 10px;
      }
      .qq-upload-size {
        margin-right: 10px;
      }
      .qq-upload-failed-text {
        display: none;
      }
      .qq-upload-delete {
        color: $colorOrange;
        cursor: pointer;
        font-family: $fontAvenirMedium;
      }
    }
    .form-button-error {
      margin-top: 10px;
      color: $colorOrange;
    }
    .form-line.file-upload {
      & {
        margin-bottom: 20px;
        cursor: pointer;
      }
    }
    .form-upload-multiple {
      position: absolute;
      top: 0;
      left: 0;
      height: 100%;
      width: 100%;
      opacity: 0;
    }
    .theme-button.outline {
      & {
        height: 35px;
        line-height: 35px;
      }
    }
  }
}

@media all and (max-width: 767px) {
  #submit-story {
    form {
      .form-line.file-upload {
        padding-bottom: 20px;
        border-bottom: 1px solid #e3dad0;
      }
      .label-type,
      .file-name {
        display: block;
        margin: 0;
      }
      .label-type {
        margin-bottom: 5px;
      }
      .file-name {
        margin-top: 15px;
      }
    }
  }
}

